\backslash
hat{k}_c.
 
\end_layout

\begin_layout Plain Layout


\backslash
label{eq:StabilityCriteria}
\end_layout

\begin_layout Plain Layout


\backslash
end{equation}
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

