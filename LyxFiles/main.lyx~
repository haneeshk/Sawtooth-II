\begin_layout Standard

\color red
We
\color inherit
 provide a systematic procedure for computing the measured force-displacement
 curves in the following Algorithm
\begin_inset ERT
status open

\begin_layout Plain Layout

~
\backslash
ref{algo:measured}
\end_layout

\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Preview

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{algorithm}[H] 
\end_layout

\begin_layout Plain Layout


\backslash
caption{Procedure for computing the measured force-displacement curves}
\backslash
label{algo:measured} 
\end_layout

\begin_layout Plain Layout


\backslash
begin{algorithmic}[1] 
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Input:} $
\backslash
Delta
\backslash
hat{w}_s$, $
\backslash
hat{k}_c$, $
\backslash
hat{F}_{
\backslash
rm eq}(
\backslash
hat{w}_0)$,} 
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Initialization:} $
\backslash
hat{w}_s(
\backslash
tau_0) = 0$, $
\backslash
hat{w}_0(
\backslash
tau_0) = 0$, $
\backslash
hat{F}(
\backslash
tau_0) = 0$.} 
\backslash
For{$i = 0,1,2,
\backslash
dotsc$} 
\end_layout

\begin_layout Plain Layout


\backslash
State Compute $
\backslash
hat{w}_s(
\backslash
tau_{i+1})=
\backslash
hat{w}_s(
\backslash
tau_i)+
\backslash
Delta w_s$ 
\backslash
State Construct~$
\backslash
hat{F}_{
\backslash
rm cant}(
\backslash
cdot;
\backslash
tau_{i+1})$ as~$
\backslash
hat{F}_{
\backslash
rm cant}(
\backslash
hat{w}_0;
\backslash
tau)=
\backslash
hat{k}_c (
\backslash
hat{w}_s(
\backslash
tau)-
\backslash
hat{w}_0)$ 
\end_layout

\begin_layout Plain Layout


\backslash
State Solve for the roots of non-linear function~$
\backslash
hat{F}_{
\backslash
rm cant}(
\backslash
cdot;
\backslash
tau_{i+1})- 
\backslash
hat{F}_{
\backslash
rm eq}(
\backslash
cdot)$ as $
\backslash
pr{
\backslash
hat{w}_0(
\backslash
tau_{i+1})}_j$, $j = 1,2,
\backslash
dots,n_{i+1}$, where $n_{i+1}$ is the total number of roots for~$
\backslash
hat{F}_{
\backslash
rm cant}(
\backslash
cdot;
\backslash
tau_{i+1})- 
\backslash
hat{F}_{
\backslash
rm eq}(
\backslash
cdot)$ 
\end_layout

\begin_layout Plain Layout


\backslash
State $
\backslash
hat{w}_0(
\backslash
tau_{i+1}) 
\backslash
leftarrow 
\backslash
pr{
\backslash
hat{w}_0(
\backslash
tau_{i+1})}_{j^*}$, where $ j^* = 
\backslash
argmin_j 
\backslash
left|
\backslash
pr{
\backslash
hat{w}_0(
\backslash
tau_{i+1})}_j -  
\backslash
hat{w}_0(
\backslash
tau_{i})
\backslash
right| $ subject to $
\backslash
hat{F}'_{
\backslash
rm cant}(
\backslash
pr{
\backslash
hat{w}_0(
\backslash
tau_{i+1})}_j;
\backslash
tau_{i+1})- 
\backslash
hat{F}'_{
\backslash
rm eq}(
\backslash
pr{
\backslash
hat{w}_0(
\backslash
tau_{i+1})}_j) < 0$  
\end_layout

\begin_layout Plain Layout


\backslash
State Compute $
\backslash
hat{F}(
\backslash
tau_{i+1}) = 
\backslash
hat{F}_{
\backslash
rm cant}(
\backslash
hat{w}_0(
\backslash
tau_{i+1}) ;
\backslash
tau_{i+1})$ 
\backslash
EndFor 
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Output:} $
\backslash
hat{w}_0(
\backslash
tau_{i+1}),
\backslash
hat{F}(
\backslash
tau_{i+1})
\backslash
;
\backslash
text{for}
\backslash
; i = 0,1,2,
\backslash
dotsc$} 
\end_layout

\begin_layout Plain Layout


\backslash
end{algorithmic} 
\end_layout

\begin_layout Plain Layout


\backslash
end{algorithm}
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

