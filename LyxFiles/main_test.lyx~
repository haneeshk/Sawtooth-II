\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Input:} $
\backslash
gamma_{
\backslash
text{sp-eq}}$, $
\backslash
hat{w}_s (
\backslash
cdot)$, $
\backslash
hat{k}_c$, $
\backslash
tau^*$, and a natural number $n$
\backslash
footnote{The parameter $n$ specifies the number of computed points on the
 measured force-displacement curve.}
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Initialization:} $
\backslash
tau_0 = 0$, ${
\backslash
hat{S}}_{0} = 1$, $
\backslash
Delta
\backslash
tau  =
\backslash
tau^*/n$}
\end_layout

\begin_layout Plain Layout


\backslash
For{$ i = 0 , 1,2,
\backslash
dotsc,n $}
\end_layout

\begin_layout Plain Layout


\backslash
State Compute $
\backslash
tau_{i+1}
\backslash
leftarrow
\backslash
tau_{i} +
\backslash
Delta
\backslash
tau$
\end_layout

\begin_layout Plain Layout


\backslash
State Construct~$R
\backslash
pr{
\backslash
cdot;
\backslash
tau_{i+1}}$ using $
\backslash
gamma_{
\backslash
text{sp-eq}}$, $
\backslash
hat{w}_s (
\backslash
cdot)$, and $
\backslash
hat{k}_c$ from~
\backslash
eqref{eq:NonDimObliqueLine}
\end_layout

\begin_layout Plain Layout


\backslash
State Solve for the roots of ~$R
\backslash
pr{
\backslash
cdot;
\backslash
tau_{i+1}}$.
 We denote those roots as $
\backslash
lsc[j]{
\backslash
hat{S}}_{i+1}$, where $j
\backslash
in
\backslash
mathcal{J}:=
\backslash
left
\backslash
{ 1,2,
\backslash
dots,n_{i+1}
\backslash
right
\backslash
}$
\end_layout

\begin_layout Plain Layout


\backslash
State $
\backslash
hat{S}_{i+1}
\backslash
leftarrow
\backslash
lsc[k^*]{
\backslash
hat{S}}_{i+1}$, where $ k^* =
\backslash
argmin_{k
\backslash
in
\backslash
mathcal{K}}
\backslash
left|
\backslash
lsc[k]{
\backslash
hat{S}}_{i+1} -  {
\backslash
hat{S}}_{i}
\backslash
right|$, $
\backslash
mathcal{K}:=
\backslash
left
\backslash
{p
\backslash
in
\backslash
mathcal{J}~|~R'(
\backslash
lsc[p]{
\backslash
hat{S}}_{i+1};
\backslash
tau_{i+1})<0
\backslash
right
\backslash
}$
\backslash
footnote{defined in~
\backslash
eqref{eq:StabilityCriteria}}
\end_layout

\begin_layout Plain Layout


\backslash
State Save  $
\backslash
pr{
\backslash
hat{w}_0^{+}
\backslash
pr{
\backslash
hat{S}_{i+1}},
\backslash
hat{F}^{+}
\backslash
pr{
\backslash
hat{S}_{i+1}}}$ as a point belonging to the measured force-displacement
 curve
\end_layout

\begin_layout Plain Layout


\backslash
EndFor
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Output:} A collection of $n$ points that belong to  the measured
 force-displacement curve
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout


\backslash
end{algorithmic}
\end_layout

\begin_layout Plain Layout


\backslash
end{minipage}
\end_layout

\begin_layout Plain Layout


\backslash
end{algorithm}
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

