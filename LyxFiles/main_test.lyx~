\backslash
begin{algorithm}[ht!]
\end_layout

\begin_layout Plain Layout


\backslash
caption{Procedure for computing the equilibrium region}
\end_layout

\begin_layout Plain Layout


\backslash
label{algo:equilibrium}
\end_layout

\begin_layout Plain Layout


\backslash
noindent
\backslash
begin{minipage}{
\backslash
textwidth}
\backslash
renewcommand
\backslash
footnoterule{}
\end_layout

\begin_layout Plain Layout


\backslash
begin{algorithmic}[1]
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Input:} $
\backslash
mu_0, A,
\backslash
lambda,
\backslash
phi,
\backslash
hat{S}^*$
\backslash
footnote{The parameter $
\backslash
hat{S}^*>1$ specifies the maximum value of $
\backslash
hat{S}$ among all the computed equilibrium configurations.}, and natural
 numbers $n_{
\backslash
hat{S}}$, and $n_{
\backslash
beta_0}$
\backslash
footnote{The parameters  $n_{
\backslash
hat{S}}$ and $n_{
\backslash
beta_0}$, respectively, specify the number of different  $
\backslash
hat{S}$ and  $
\backslash
beta_0$ values among the computed equilibrium configurations.}
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Initialization:} $
\backslash
hat{S} = 1$, $
\backslash
hat{P}_n = 0$, $
\backslash
hat{w}_0 = 0$, $
\backslash
hat{F} = 0$, $
\backslash
Delta
\backslash
hat{S} =
\backslash
hat{S}^*/n_{
\backslash
hat{S}}$}
\end_layout

\begin_layout Plain Layout


\backslash
For{$
\backslash
hat{S} = 1,1+
\backslash
Delta
\backslash
hat{S},1+2
\backslash
Delta
\backslash
hat{S},
\backslash
dotsc,
\backslash
hat{S}^*$}
\end_layout

\begin_layout Plain Layout


\backslash
State Compute~$
\backslash
mu
\backslash
pr{
\backslash
hat{S}}
\backslash
leftarrow
\backslash
mu_{0}
\backslash
left(1+A
\backslash
cos
\backslash
left(
\backslash
pi
\backslash
hat{S}/
\backslash
lambda+
\backslash
phi
\backslash
right)
\backslash
right)$
\end_layout

\begin_layout Plain Layout


\backslash
State Compute~$
\backslash
beta_{0}^+
\backslash
leftarrow
\backslash
text{arccot}
\backslash
pr{-
\backslash
mu
\backslash
pr{
\backslash
hat{S}}} $ and $
\backslash
beta_{0}^-
\backslash
leftarrow
\backslash
text{arccot}
\backslash
pr{
\backslash
mu
\backslash
pr{
\backslash
hat{S}}} $
\end_layout

\begin_layout Plain Layout


\backslash
State Compute~$
\backslash
Delta
\backslash
beta_0
\backslash
leftarrow
\backslash
pr{
\backslash
beta_0^+ -
\backslash
beta_0^-}/n_{
\backslash
beta_0}$
\backslash
For{$
\backslash
beta_{0}  =
\backslash
beta_0^-,
\backslash
beta_0^-+
\backslash
Delta
\backslash
beta_0,
\backslash
beta_0^-+2
\backslash
Delta
\backslash
beta_0,
\backslash
dotsc
\backslash
beta_0^+$}
\end_layout

\begin_layout Plain Layout


\backslash
State Solve for~$
\backslash
hat{P}_{n}$ as the smallest root of~$f(
\backslash
cdot;
\backslash
hat{S},
\backslash
beta_{0})$
\backslash
footnote{defined in~
\backslash
eqref{eq:ClosingEquation}}
\end_layout

\begin_layout Plain Layout


\backslash
State Construct~$
\backslash
theta(
\backslash
cdot;
\backslash
idp)$ using $
\backslash
hat{S}$, $
\backslash
hat{P}_{n}$,and $
\backslash
beta_0$ from~
\backslash
eqref{eq:ThetaSol}
\end_layout

\begin_layout Plain Layout


\backslash
State Determine~$
\backslash
hat{w}_0(
\backslash
idp)$ and~$
\backslash
hat{F}(
\backslash
idp)$ from~
\backslash
eqref{eq:w0hat},~
\backslash
eqref{eq:FhatSol},~
\backslash
eqref{eq:omegaNew} and~
\backslash
eqref{eq:Theta0}
\end_layout

\begin_layout Plain Layout


\backslash
State Save the points $(
\backslash
hat{S},
\backslash
hat{w}_0(
\backslash
idp))$, $(
\backslash
hat{S},
\backslash
hat{F}(
\backslash
idp))$, and $(
\backslash
hat{w}_0(
\backslash
idp),
\backslash
hat{F}(
\backslash
idp))$ as respective members of the equilibrium regions in the $
\backslash
hat{S}$-$
\backslash
hat{w}_0$, $
\backslash
hat{S}$-$
\backslash
hat{F}$, and $
\backslash
hat{w}_0$-$
\backslash
hat{F}$ spaces
\end_layout

\begin_layout Plain Layout


\backslash
EndFor
\end_layout

\begin_layout Plain Layout


\backslash
EndFor
\end_layout

\begin_layout Plain Layout


\backslash
State{
\backslash
textbf{Output:} A collection of $n_{
\backslash
hat{S}}
\backslash
times n_{
\backslash
beta_0}$ equilibrium points in each of the  $
\backslash
hat{S}$-$
\backslash
hat{w}_0$, $
\backslash
hat{S}$-$
\backslash
hat{F}$, and $
\backslash
hat{w}_0$-$
\backslash
hat{F}$ spaces}
\end_layout

\begin_layout Plain Layout


\backslash
end{algorithmic}
\end_layout

